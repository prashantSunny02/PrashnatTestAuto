<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="Opencart App Test Regression Suite" thread-count="2"
	parallel="tests" >


	<listeners>
		<listener
			class-name="com.qa.opencart.listners.ExtentReportListeners" />
	 <listener
			class-name="com.qa.opencart.listners.TestAllureListener" />
		<!-- <listener
			class-name="com.qa.opencart.listeners.AnnotationTransformer" /> -->
	</listeners>


	<test name="Login Page Test">
		

		<classes>
			<class name="com.qa.opencart.test.LoginPageTest" />
		</classes>
	</test>



 	<test name="Accounts Page Test">
		

		<classes>
			<class name="com.qa.opencart.test.AccountPageTest" />
		</classes>
	</test>


